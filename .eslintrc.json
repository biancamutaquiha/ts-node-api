{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "import",
        "jest"
    ],
    "rules": {
        "max-len": [
            "warn",
            {
                "code": 95
            }
        ]
    }
}